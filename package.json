{
  "name": "snaly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn test:unit && gulp build-icon-font && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "lighthouse": "mkdirp lighthouse && lighthouse https://pierre-m.github.io/snaly --quiet --view --output html --output-path ./lighthouse/report.html",
    "lighthouse:clean": "rm -rf lighthouse",
    "postinstall": "yarn run prepare-assets",
    "prepare-assets": "gulp build-icon-font"
  },
  "dependencies": {
    "@sentry/browser": "5.12.1",
    "@sentry/integrations": "5.12.0",
    "@types/lodash": "4.14.149",
    "axios": "0.19.2",
    "core-js": "3.6.4",
    "date-fns": "2.9.0",
    "lighthouse": "5.6.0",
    "lodash": "4.17.15",
    "portal-vue": "2.1.7",
    "querystring": "0.2.0",
    "register-service-worker": "1.6.2",
    "shake.js": "1.2.2",
    "tailwindcss": "1.2.0",
    "tailwindcss-filters": "3.0.0",
    "tsyringe": "4.0.1",
    "v-click-outside": "3.0.1",
    "v-hotkey": "0.8.0",
    "v-scroll-lock": "1.1.0",
    "vue": "2.6.11",
    "vue-class-component": "7.2.2",
    "vue-i18n": "8.15.3",
    "vue-property-decorator": "8.3.0",
    "vue2-transitions": "0.3.0",
    "vuex": "3.1.2",
    "vuex-class": "0.3.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "2.0.6",
    "@types/jest": "24.9.0",
    "@vue/cli-plugin-babel": "~4.2.2",
    "@vue/cli-plugin-e2e-cypress": "~4.2.2",
    "@vue/cli-plugin-eslint": "~4.2.2",
    "@vue/cli-plugin-pwa": "~4.2.2",
    "@vue/cli-plugin-typescript": "~4.2.2",
    "@vue/cli-plugin-unit-jest": "~4.2.2",
    "@vue/cli-plugin-vuex": "~4.2.2",
    "@vue/cli-service": "~4.2.2",
    "@vue/eslint-config-prettier": "5.1.0",
    "@vue/eslint-config-typescript": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "async": "3.1.1",
    "autoprefixer": "9.7.4",
    "eslint": "5.16.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-vue": "5.2.3",
    "gulp": "4.0.2",
    "gulp-consolidate": "0.2.0",
    "gulp-iconfont": "10.0.3",
    "husky": "4.2.1",
    "jest-mock-axios": "3.2.0",
    "prettier": "1.19.1",
    "push-dir": "0.4.1",
    "reflect-metadata": "0.1.13",
    "sass": "1.25.0",
    "sass-loader": "8.0.2",
    "simulant": "0.2.2",
    "typescript": "~3.7.5",
    "vue-template-compiler": "2.6.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test:unit"
    }
